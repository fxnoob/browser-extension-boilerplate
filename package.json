{
  "name": "chrome-extension-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "homepage": "https://fxnoob.github.io/chrome-extension-boilerplate",
  "scripts": {
    "build:css": "tailwindcss build src/styles/style.css -o dist/style.css",
    "watch:css": "yarn build:css && chokidar 'src/styles/*.css' -c 'yarn build:css'",
    "watch:js": "webpack --watch --config webpack.config.js --mode=development",
    "build:js": "webpack  --config webpack.config.js --mode=production",
    "rm": "rm -rf dist/",
    "start": "yarn dev",
    "dev": "concurrently --kill-others 'yarn watch:css' 'yarn watch:js'",
    "build": "yarn rm && concurrently 'yarn build:css' 'yarn build:js'",
    "jsdoc": "jsdoc src -r",
    "deploy_jsdoc": "yarn jsdoc &&  gh-pages -d out",
    "test": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn build"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.5.1",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "esbuild-loader": "^2.4.0",
    "eslint": "^7.6.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.5",
    "eslint-watch": "^7.0.0",
    "file-loader": "^4.2.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "image-webpack-loader": "^5.0.0",
    "jsdoc": "^3.6.3",
    "prettier": "1.18.2",
    "pretty-quick": "^2.0.0",
    "react-hot-loader": "^4.12.10",
    "style-loader": "^1.2.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.5.5",
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "@tailwindcss/ui": "^0.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-frame-component": "^4.1.3",
    "tailwindcss": "^2.0.0"
  }
}
